# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp

# Ignore MP configuration
#config/initializers/ZigzagCommerce.rb

# Ignore Cloudinary configuration file
#config/locales/cloudinary.yml

# Ignore application configuration files
#config/locales/application.rb
#config/locales/secrets.yml